{
	"name": "silverstripe/installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.3.3",
		"silverstripe/cms": "3.2.1",
		"silverstripe/framework": "3.2.1",
		"silverstripe/reports": "3.2.1",
		"silverstripe/siteconfig": "3.2.1",
		"axllent/silverstripe-less": "dev-master",
		"phpmailer/phpmailer": "~5.2",
		"mailin-api/mailin-api-php": "^1.0",
		"silverstripe-australia/gridfieldextensions": "dev-master",
		"silverstripe/staticpublishqueue": "dev-master",
		"firebrandhq/silverstripe-excel-export": "^0.0.1",
		"markguinn/silverstripe-gridfieldmultiselect": "^0.0.2",
		"unisolutions/silverstripe-copybutton": "^1.0"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~3.7"
